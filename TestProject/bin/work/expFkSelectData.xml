<?xml version="1.0" encoding="UTF-8" ?>

<main>
	<elem name="A1" type="buildData" >
		<select>a1.anali_konto</select>	
		
		<from>gmsl_granali1 a1</from>
		<from>gmsl_tow tow</from>
		<from>gmsl_towlok tlok</from>
		
		<where>nag.anali1 = a1.anali_kod and nag.adali1 is not null and tow.anali1 = a1.anaki_kod</where>	
		<group>a1.anali_konto</group>	
		<having></having>
		<order>a1.anali_konto</order>	
	</elem>
	<elem name="A2" type="buildData" >
		<select>a2.anali_konto</select>	
		<from>
			<from-elem>gmsl_granali2 a2</from-elem>
			<from-elem>gmsl_tow tow</from-elem>
			<from-elem>gmsl_towlok tlok</from-elem>
		</from>	
		<where>nag.anali2 = a2.anali_kod and nag.adali2 is not null and tow.anali2 = a2.anaki_kod</where>	
		<group>a2.anali_konto</group>	
		<having></having>
		<order>a2.anali_konto</order>	
	</elem>
	
	
	
</main>